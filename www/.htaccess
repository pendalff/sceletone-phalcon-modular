AddDefaultCharset UTF-8

php_flag display_errors on

<IfModule mod_env.c>
    SetEnv APPLICATION_ENV "dev"
</IfModule>

<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^(.*)$ index.php?_url=/$1 [QSA,L]
</IfModule>